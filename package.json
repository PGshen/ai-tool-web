{
  "name": "hello",
  "version": "0.1.0",
  "private": true,
  "description": "A dashboard scaffolding based on Vue.js 3.x & TypeScript created by Vite.",
  "keywords": [
    "Hello",
    "dashboard",
    "vue",
    "vuejs",
    "vue3",
    "typescript",
    "vite",
    "zce"
