    "highlight.js": "^11.8.0",
    "markdown-it": "^13.0.1",
    "markdown-it-highlightjs": "^4.0.1",
    "naive-ui": "2.34.4",
    "pinia": "^2.0.12",
    "pinia-plugin-persistedstate": "^3.1.0",
    "vue": "^3.2.25",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@commitlint/cli": "16.0.0",
    "@commitlint/config-conventional": "16.0.0",
